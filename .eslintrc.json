{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "globals": {
    "window": false,
    "d3": false,
    "document": false,
    "noUiSlider": false,
    "colorbrewer": false,
    "topojson": false,
    "Blob": false,
    "navigator": false
  },
  "rules": {
    "no-use-before-define": 0,
    "no-mixed-operators": 0,
    "no-console": 0,
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ],
    "func-names": [
      "off",
      "as-needed"
    ],
    "semi": "error",
    "linebreak-style": "off"
  },
  "plugins": [
    "prettier"
  ]
}